<core:FragmentDefinition xmlns="sap.m"
    xmlns:core="sap.ui.core"
    xmlns:cust="shop.bookshop.controls">
    <ColumnListItem id="_IDGenColumnListItem1">
        <cells>
            <HBox id="_IDGenHBox">
                <Input id="_IDGenInput" value="{amount}" change="onChanged"/>
                <Input id="currInput" value="{currency_code}" />
                <MultiInput id="salesPersons" tokens="{salesPersons}">
                    <tokens>
                        <Token id="salesPersonToken" key="{ID}" text="{name}" delete="onSalesPersonRemoved"/>
                    </tokens>
                </MultiInput>
            </HBox>
        </cells>
    </ColumnListItem>
</core:FragmentDefinition>